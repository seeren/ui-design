  "dependencies": {
	"ui-dialog": "latest"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "node-sass": "latest",
    "watchify": "latest"
  },
  "repository" : {
    "type" : "git",
	"url" : "https://github.com/seeren/ui-design"
  }
}