{
  "name": "ui-design",
  "version": "1.0.4",
  "description": "Sass files for ui components",
  "keywords": ["ui","javascript", "sass", "css"],
  "main": "./sass/index.js",
  "scripts": {
    "dev": "npm run sync | npm run sassify",
    "sassify": "./node_modules/.bin/node-sass --watch ./sass/index.scss ./dist/ui-design.css",
    "sync": "./node_modules/.bin/browser-sync start --server --files \"./dist/ui-design.css\""
  },
