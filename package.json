{
  "name": "ui-design",
  "description": "Mobile first component helper",
  "version": "2.1.5",
  "keywords": [
    "ui",
    "html",
    "css",
    "javascript",
    "helper"
  ],
  "homepage": "https://seeren.github.io/ui-design/",
  "author": "Cyril Ichti",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --recursive test/unit",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --recursive test/unit",
    "test:coveralls": "npm run test:coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "style": "dist/index.css",
  "sass": "src/index",
  "main": "src/index",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/register": "latest",
    "babel-loader": "latest",
    "babel-plugin-istanbul": "latest",
    "browser-sync": "latest",
    "browser-sync-webpack-plugin": "latest",
    "chai": "latest",
    "coveralls": "latest",
    "cross-env": "latest",
    "css-loader": "latest",
    "file-loader": "latest",
    "jsdom": "latest",
    "mini-css-extract-plugin": "latest",
    "mocha": "latest",
    "mocha-lcov-reporter": "latest",
    "node-sass": "latest",
    "nyc": "latest",
    "sass-loader": "latest",
    "sinon": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "engines": {
    "node": ">=10"
  }
}
